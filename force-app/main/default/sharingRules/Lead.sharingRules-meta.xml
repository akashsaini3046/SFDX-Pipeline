            <group>All_Crowley_Internal_Users</group>
        </sharedTo>
        <sharedFrom>
            <group>Sales_Agent_Group</group>
        </sharedFrom>
    </sharingOwnerRules>
    <sharingOwnerRules>
        <fullName>Sharing_Leads_with_All_Crowley_Internal_Users</fullName>
