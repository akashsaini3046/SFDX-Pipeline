        <accessLevel>Edit</accessLevel>
        <label>Queue Based Sharing Rule</label>
        <sharedTo>
            <group>All_Crowley_Internal_Users</group>
        </sharedTo>
        <criteriaItems>
            <field>OwnerId</field>
            <operation>startsWith</operation>
            <value>00G</value>
        </criteriaItems>
    </sharingCriteriaRules>
    <sharingOwnerRules>
        <fullName>Sharing_Lead_Owned_By_Sales_Agent</fullName>
