        <label>Sharing Leads with All Crowley Internal Users</label>
        <sharedTo>
            <group>All_Crowley_Internal_Users</group>
        </sharedTo>
        <sharedFrom>
            <group>All_Crowley_Internal_Users</group>
        </sharedFrom>
    </sharingOwnerRules>
