    <rules>
        <fullName>Arrival Notice Case</fullName>
        <actions>
            <name>Arrival_Notice_Case_update</name>
            <type>FieldUpdate</type>
        </actions>
        <active>true</active>
        <booleanFilter>(1 OR 3) AND 2</booleanFilter>
        <criteriaItems>
            <field>Case.RecordTypeId</field>
            <operation>equals</operation>
            <value>Customer Inquiry</value>
        </criteriaItems>
        <criteriaItems>
            <field>Case.OwnerId</field>
            <operation>contains</operation>
            <value>Arrival Notices</value>
        </criteriaItems>
        <criteriaItems>
            <field>Case.RecordTypeId</field>
            <operation>equals</operation>
            <value>Create Customer Inquiry</value>
        </criteriaItems>
        <triggerType>onCreateOnly</triggerType>
    </rules>
    <rules>
        <fullName
    >Auto-Response Arrival Notices English New Case on Closed Cases</fullName>
        <actions>
            <name
      >Send_an_english_email_to_customer_Arrival_Notices_after_closed_new_cases</name>
            <type>Alert</type>
        </actions>
        <active>true</active>
        <formula
    >IF(   AND(ISNULL(ContactId), ISBLANK(ContactId)),   false,   AND(      OR(RecordType.Id = $Label.Create_Customer_Inquiry_Record_Type,RecordType.Id =$Label.Customer_Inquiry_Record_Type ),     OR(ISPICKVAL( Contact.Language_Preference__c , &apos;English&apos;), ISNULL( TEXT(Contact.Language_Preference__c ) ), ISBLANK(TEXT(Contact.Language_Preference__c ))  ),      AND( NOT(ISNULL( Original_Case_2__c )) , NOT(ISBLANK( Original_Case_2__c )) ),     OR(        NOT( ISNULL( Contact.Email )),        NOT( ISNULL(SuppliedEmail))     ),       CONTAINS(Owner:Queue.QueueName, &apos;Arrival Notice&apos;)       ) )</formula>
        <triggerType>onCreateOnly</triggerType>
    </rules>
