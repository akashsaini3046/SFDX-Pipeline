<apex:page
  controller="CC_BillOfLadingVFController"
  showHeader="false"
  sidebar="false"
  renderAs="pdf"
  applyBodyTag="false"
>
  <head>
    <style type="text/css" media="print">
      * {
          font-family: sans-serif;
          "

      }

      @page {
          @top-center {
              content: element(header);
          }

          @bottom-left {
              content: element(footer);
          }
      }

      div.header {
          text-align: center;
          position: running(header);
          width: 100%;
      }

      div.footer {
          display: block;
          padding: 5px;
          position: running(footer);
      }

      .pagenumber:before {
          content: counter(page);
      }

      .pagecount:before {
          content: counter(pages);
      }

      .centered {
          text-align: center;
      }

      table {
          border-collapse: collapse;
          /* Remove cell spacing */
          width: 100%;
      }

      table,
      th,
      td {
          vertical-align: top;

      }

      table th,
      table td {
          padding: 0px;
          /* Apply cell padding */
      }

      .main-table {
          margin: 0px auto;
      }

      .key-border-around {
          border: solid 1px #ccc;
      }

      .key-border-around-no-bb {
          border-right: solid 1px #ccc;
          border-left: solid 1px #ccc;
      }

      .padding-5-around {
          padding: 5px;
      }

      .p-padding-5-around {
          padding: 5px;
      }

      .bol-rel-label {
          color: #6a6e6f;
          font-weight: bold;
      }

      .row-left,
      .row-right {
          width: 50%;
      }

      .row-full {
          width: 100%;
      }

      .no-wrap {
          white-space: nowrap;
      }
    </style>
  </head>

